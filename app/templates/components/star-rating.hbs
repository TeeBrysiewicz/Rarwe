{{#if hasBlock}}
  {{yield this.stars}}
{{else}}

  {{#each this.stars as |star|}}
    <button type="button" class="rating-star" {{on "click" (fn this.setRating star.rating)}}>
      {{#if star.isFull}}
        {{fa-icon "star"}}
      {{else}}
        {{fa-icon "star-0"}}
      {{/if}}
    </button>
  {{/each}}
{{/if}}
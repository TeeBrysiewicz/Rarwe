{{#each this.stars as |star|}}
  <button
    type="button"
    class="cursor-pointer p-0 m-0 text-yellow"
    data-test-rr={{concat "star-rating-" star.rating}}
    {{on "click" (fn this.setRating star.rating)}}
  >
    {{#if star.isFull}}
      {{fa-icon "star"}}
    {{else}}
      {{fa-icon "star-o"}}
    {{/if}}
  </button>
{{/each}}